{
  "version": "2.0",
  "app_name": "API",
  "stages": {
    "UserManagementBackendSandbox/API": {
      "api_gateway_stage": "v1",
      "lambda_functions": {
        "api_handler": {
          "manage_iam_role": false,
          "iam_role_arn": "${Token[TOKEN.208]}",
          "environment_variables": {
            "DYNAMODB_TABLE_NAME": "${Token[TOKEN.200]}"
          },
          "reserved_concurrency": 1
        }
      }
    },
    "UserManagementBackendToolchain/Pipeline/Production/UserManagementBackend/API": {
      "api_gateway_stage": "v1",
      "lambda_functions": {
        "api_handler": {
          "manage_iam_role": false,
          "iam_role_arn": "${Token[TOKEN.256]}",
          "environment_variables": {
            "DYNAMODB_TABLE_NAME": "${Token[TOKEN.248]}"
          },
          "reserved_concurrency": 10
        }
      }
    }
  }
}